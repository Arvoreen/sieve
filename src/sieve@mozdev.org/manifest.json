{
  "name" : "Sieve",
  "description" : "Configures serverside sieve email filters",
  "version" : "0.1",
  "manifest_version" : 2,
  
  "app" : {
    "background" : {
      "scripts" : ["webapp.js"]
    }
  },
  
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "browser"
  ],
  
  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  }
}
